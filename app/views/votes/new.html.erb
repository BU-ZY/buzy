<% provide(:title, 'Cast a Busyness Vote') %>
<h1>Sign up</h1>

<div class="row">
  <div class="span6 offset3">
    <%= simple_form_for(@vote) do |f| %>

      <%= f.label :score %>
      <%= f.text_field :score %>

     <p>
 		<%= f.label :Place %><br />
  		<%=f.input :age, collection: 18..60, prompt: "Select your age"%>
	</p>

      <%= f.submit "Create Vote", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>