<%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>

<div class="container">

<div class="center"><h1>Places</h1></div>

<div class="row">
	<div id="places">
		<% @places.each do |place| %>
			<a href="\places/<%= place.id %>/60">
				<div class="place"><%= place.name%></div>
			</a>
		<% end %>
	</div>
</div>

<div class="center">
	<%= link_to "add a place", '/places/new', class: "btn btn-large btn-primary" %> 
</div>
</div>